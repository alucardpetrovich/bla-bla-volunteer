{
  "presets": ["@babel/preset-react"],
  "plugins": [
    [
      "formatjs",
      {
        "idInterpolationPattern": "[sha512:contenthash:base64:6]",
        "ast": true
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "react-intl-auto",
      {
        "removePrefix": "locales/"
      }
    ],
    "@formatjs/ts-transformer"
  ]
}
